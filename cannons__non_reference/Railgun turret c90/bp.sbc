/***************************************************************************************************
* 入口函数 Main()
***************************************************************************************************/
void Main(string str_arg, UpdateType type_update)
{
	switch (type_update)
	{
		case UpdateType.Terminal:
		case UpdateType.Trigger:
		case UpdateType.Script:
			{
				run_command(str_arg);
			}
			break;
		case UpdateType.Update1:
		case UpdateType.Update10:
		case UpdateType.Update100:
			{
				update_script();
			}
			break;
	}
}

