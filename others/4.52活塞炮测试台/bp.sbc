				Alignment = TextAlignment.CENTER,
			};
			frame.Add(element__red_annulus);

			//绘图元素 红色叉
			MySprite element__red_cross = new MySprite()
			{
				Type = SpriteType.TEXTURE,
				Data = "Cross",
				Position = size * (new Vector2(0.5f, 0.4f)),
				Size = size * 0.4f,
				Color = new Color(255, 0, 0),
				Alignment = TextAlignment.CENTER,
